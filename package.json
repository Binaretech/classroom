  },
  "name": "classroom",
  "packageManager": "npm@8.11.0",
  "workspaces": [
    "web",
    "docs",
    "nestjs",
    "packages/*"
  ],
  "overrides": {
    "web": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    },
    "classroom-docs": {
      "react": "^17.0.2",
      "react-dom": "^17.0.2"
    }
  }
